/* Add a black background color to the top navigation */
.topnav {
   overflow: hidden;
   background-color: #6B3CC9;
   width: 1440px;
   height: 77px;
   align-items: center;
   gap: 0px;
   opacity: 0px;

   
}

/* Style the links inside the navigation bar */
.topnav a {
   display: flex;
   float: right; 
   display: block;
   color: #f2f2f2;
   padding: 14px 16px;
   text-decoration: none;
   Top: 215px;
   Left: 77px;
 

 
}

.topnav a:hover {
   background-color: #ddd; 
   color: black;
}

.topnav a.active {
   background-color: #4caf50; 
   color: white;
}


